{
  "name": "@minicalen/server",
  "version": "1.0.0",
  "description": "MiniCalen backend server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"npx tsx src/index.ts\" --watch src --ext ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "globals": "^15.9.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.1"
